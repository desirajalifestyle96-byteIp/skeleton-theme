{% doc %}
  @prompt
    A magazine-style editorial layout
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-magazine-layout-{{ ai_gen_id }} {
    max-width: {{ block.settings.max_width }}px;
    margin: 0 auto;
    padding: {{ block.settings.section_padding }}px 20px;
    background-color: {{ block.settings.background_color }};
  }

  .ai-magazine-layout__grid-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(12, 1fr);
    gap: {{ block.settings.grid_gap }}px;
  }

  .ai-magazine-layout__hero-{{ ai_gen_id }} {
    grid-column: span 12;
    position: relative;
    overflow: hidden;
    border-radius: {{ block.settings.image_border_radius }}px;
    min-height: 400px;
  }

  .ai-magazine-layout__hero-image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
    display: block;
  }

  .ai-magazine-layout__hero-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    min-height: 400px;
    background-color: #f4f4f4;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
  }

  .ai-magazine-layout__hero-placeholder-{{ ai_gen_id }} svg {
    width: 100%;
    height: 100%;
    max-width: 500px;
    max-height: 500px;
  }

  .ai-magazine-layout__hero-content-{{ ai_gen_id }} {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    padding: 40px;
    background: linear-gradient(to top, rgba(0, 0, 0, 0.8), transparent);
    color: #ffffff;
  }

  .ai-magazine-layout__hero-category-{{ ai_gen_id }} {
    font-size: 12px;
    text-transform: uppercase;
    letter-spacing: 1.5px;
    margin-bottom: 8px;
    color: {{ block.settings.accent_color }};
    font-weight: 600;
  }

  .ai-magazine-layout__hero-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.hero_title_size }}px;
    line-height: 1.2;
    margin: 0 0 12px;
    font-weight: 700;
  }

  .ai-magazine-layout__hero-excerpt-{{ ai_gen_id }} {
    font-size: 16px;
    line-height: 1.6;
    margin: 0 0 16px;
    opacity: 0.9;
  }

  .ai-magazine-layout__hero-meta-{{ ai_gen_id }} {
    font-size: 14px;
    opacity: 0.8;
  }

  .ai-magazine-layout__secondary-{{ ai_gen_id }} {
    grid-column: span 6;
    display: flex;
    flex-direction: column;
  }

  .ai-magazine-layout__secondary-image-wrapper-{{ ai_gen_id }} {
    position: relative;
    overflow: hidden;
    border-radius: {{ block.settings.image_border_radius }}px;
    margin-bottom: 16px;
    aspect-ratio: 16 / 10;
  }

  .ai-magazine-layout__secondary-image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
    display: block;
  }

  .ai-magazine-layout__secondary-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    background-color: #f4f4f4;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-magazine-layout__secondary-placeholder-{{ ai_gen_id }} svg {
    width: 100%;
    height: 100%;
    max-width: 300px;
    max-height: 300px;
  }

  .ai-magazine-layout__secondary-category-{{ ai_gen_id }} {
    font-size: 11px;
    text-transform: uppercase;
    letter-spacing: 1.2px;
    margin-bottom: 8px;
    color: {{ block.settings.accent_color }};
    font-weight: 600;
  }

  .ai-magazine-layout__secondary-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.secondary_title_size }}px;
    line-height: 1.3;
    margin: 0 0 12px;
    font-weight: 700;
    color: {{ block.settings.text_color }};
  }

  .ai-magazine-layout__secondary-excerpt-{{ ai_gen_id }} {
    font-size: 14px;
    line-height: 1.6;
    margin: 0 0 12px;
    color: {{ block.settings.text_color }};
    opacity: 0.8;
  }

  .ai-magazine-layout__secondary-meta-{{ ai_gen_id }} {
    font-size: 13px;
    color: {{ block.settings.text_color }};
    opacity: 0.6;
    margin-top: auto;
  }

  .ai-magazine-layout__tertiary-{{ ai_gen_id }} {
    grid-column: span 4;
    display: flex;
    gap: 16px;
  }

  .ai-magazine-layout__tertiary-image-wrapper-{{ ai_gen_id }} {
    flex-shrink: 0;
    width: 100px;
    height: 100px;
    border-radius: {{ block.settings.image_border_radius }}px;
    overflow: hidden;
  }

  .ai-magazine-layout__tertiary-image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
    display: block;
  }

  .ai-magazine-layout__tertiary-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    background-color: #f4f4f4;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-magazine-layout__tertiary-placeholder-{{ ai_gen_id }} svg {
    width: 60px;
    height: 60px;
  }

  .ai-magazine-layout__tertiary-content-{{ ai_gen_id }} {
    flex: 1;
    display: flex;
    flex-direction: column;
  }

  .ai-magazine-layout__tertiary-category-{{ ai_gen_id }} {
    font-size: 10px;
    text-transform: uppercase;
    letter-spacing: 1px;
    margin-bottom: 6px;
    color: {{ block.settings.accent_color }};
    font-weight: 600;
  }

  .ai-magazine-layout__tertiary-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.tertiary_title_size }}px;
    line-height: 1.4;
    margin: 0 0 8px;
    font-weight: 600;
    color: {{ block.settings.text_color }};
  }

  .ai-magazine-layout__tertiary-meta-{{ ai_gen_id }} {
    font-size: 12px;
    color: {{ block.settings.text_color }};
    opacity: 0.6;
    margin-top: auto;
  }

  .ai-magazine-layout__divider-{{ ai_gen_id }} {
    grid-column: span 12;
    height: 1px;
    background-color: {{ block.settings.text_color }};
    opacity: 0.1;
    margin: {{ block.settings.divider_spacing }}px 0;
  }

  @media screen and (max-width: 989px) {
    .ai-magazine-layout__secondary-{{ ai_gen_id }} {
      grid-column: span 12;
    }

    .ai-magazine-layout__tertiary-{{ ai_gen_id }} {
      grid-column: span 6;
    }

    .ai-magazine-layout__hero-title-{{ ai_gen_id }} {
      font-size: calc({{ block.settings.hero_title_size }}px * 0.7);
    }

    .ai-magazine-layout__secondary-title-{{ ai_gen_id }} {
      font-size: calc({{ block.settings.secondary_title_size }}px * 0.85);
    }
  }

  @media screen and (max-width: 749px) {
    .ai-magazine-layout-{{ ai_gen_id }} {
      padding: {{ block.settings.section_padding | times: 0.6 }}px 16px;
    }

    .ai-magazine-layout__grid-{{ ai_gen_id }} {
      gap: {{ block.settings.grid_gap | times: 0.7 }}px;
    }

    .ai-magazine-layout__hero-{{ ai_gen_id }} {
      min-height: 300px;
    }

    .ai-magazine-layout__hero-content-{{ ai_gen_id }} {
      padding: 24px;
    }

    .ai-magazine-layout__hero-title-{{ ai_gen_id }} {
      font-size: calc({{ block.settings.hero_title_size }}px * 0.5);
    }

    .ai-magazine-layout__secondary-title-{{ ai_gen_id }} {
      font-size: calc({{ block.settings.secondary_title_size }}px * 0.75);
    }

    .ai-magazine-layout__tertiary-{{ ai_gen_id }} {
      grid-column: span 12;
    }

    .ai-magazine-layout__tertiary-image-wrapper-{{ ai_gen_id }} {
      width: 80px;
      height: 80px;
    }
  }
{% endstyle %}

<div class="ai-magazine-layout-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-magazine-layout__grid-{{ ai_gen_id }}">
    <article class="ai-magazine-layout__hero-{{ ai_gen_id }}">
      {% if block.settings.hero_image %}
        <img
          src="{{ block.settings.hero_image | image_url: width: 1600 }}"
          alt="{{ block.settings.hero_image.alt | escape }}"
          loading="lazy"
          width="{{ block.settings.hero_image.width }}"
          height="{{ block.settings.hero_image.height }}"
          class="ai-magazine-layout__hero-image-{{ ai_gen_id }}"
        >
      {% else %}
        <div class="ai-magazine-layout__hero-placeholder-{{ ai_gen_id }}">
          {{ 'hero-apparel-1' | placeholder_svg_tag }}
        </div>
      {% endif %}
      <div class="ai-magazine-layout__hero-content-{{ ai_gen_id }}">
        {% if block.settings.hero_category != blank %}
          <div class="ai-magazine-layout__hero-category-{{ ai_gen_id }}">{{ block.settings.hero_category }}</div>
        {% endif %}
        {% if block.settings.hero_title != blank %}
          <h2 class="ai-magazine-layout__hero-title-{{ ai_gen_id }}">{{ block.settings.hero_title }}</h2>
        {% endif %}
        {% if block.settings.hero_excerpt != blank %}
          <div class="ai-magazine-layout__hero-excerpt-{{ ai_gen_id }}">{{ block.settings.hero_excerpt }}</div>
        {% endif %}
        {% if block.settings.hero_author != blank or block.settings.hero_date != blank %}
          <div class="ai-magazine-layout__hero-meta-{{ ai_gen_id }}">
            {% if block.settings.hero_author != blank %}{{ block.settings.hero_author }}{% endif %}
            {% if block.settings.hero_author != blank and block.settings.hero_date != blank %} • {% endif %}
            {% if block.settings.hero_date != blank %}{{ block.settings.hero_date }}{% endif %}
          </div>
        {% endif %}
      </div>
    </article>

    {% if block.settings.show_divider %}
      <div class="ai-magazine-layout__divider-{{ ai_gen_id }}"></div>
    {% endif %}

    <article class="ai-magazine-layout__secondary-{{ ai_gen_id }}">
