{% schema %}
{
  "name": "Magazine editorial layout",
  "tag": null,
  "settings": [
    {
      "type": "header",
      "content": "Layout"
    },
    {
      "type": "range",
      "id": "max_width",
      "min": 800,
      "max": 1600,
      "step": 100,
      "unit": "px",
      "label": "Maximum width",
      "default": 1200
    },
    {
      "type": "range",
      "id": "section_padding",
      "min": 0,
      "max": 100,
      "step": 5,
      "unit": "px",
      "label": "Section padding",
      "default": 60
    },
    {
      "type": "range",
      "id": "grid_gap",
      "min": 10,
      "max": 60,
      "step": 5,
      "unit": "px",
      "label": "Grid gap",
      "default": 30
    },
    {
      "type": "range",
      "id": "image_border_radius",
      "min": 0,
      "max": 20,
      "step": 2,
      "unit": "px",
      "label": "Image border radius",
      "default": 4
    },
    {
      "type": "checkbox",
      "id": "show_divider",
      "label": "Show dividers",
      "default": true
    },
    {
      "type": "range",
      "id": "divider_spacing",
      "min": 10,
      "max": 60,
      "step": 5,
      "unit": "px",
      "label": "Divider spacing",
      "default": 30
    },
    {
      "type": "header",
      "content": "Colors"
    },
    {
      "type": "color",
      "id": "background_color",
      "label": "Background",
      "default": "#ffffff"
    },
    {
      "type": "color",
      "id": "text_color",
      "label": "Text",
      "default": "#1a1a1a"
    },
    {
      "type": "color",
      "id": "accent_color",
      "label": "Accent",
      "default": "#d4af37"
    },
    {
      "type": "header",
      "content": "Typography"
    },
    {
      "type": "range",
      "id": "hero_title_size",
      "min": 24,
      "max": 72,
      "step": 2,
      "unit": "px",
      "label": "Hero title size",
      "default": 48
    },
    {
      "type": "range",
      "id": "secondary_title_size",
      "min": 16,
      "max": 40,
      "step": 2,
      "unit": "px",
      "label": "Secondary title size",
      "default": 24
    },
    {
      "type": "range",
      "id": "tertiary_title_size",
      "min": 12,
      "max": 24,
      "step": 1,
      "unit": "px",
      "label": "Tertiary title size",
      "default": 16
    },
    {
      "type": "header",
      "content": "Hero article"
    },
    {
      "type": "image_picker",
      "id": "hero_image",
      "label": "Image"
    },
    {
      "type": "text",
      "id": "hero_category",
      "label": "Category",
      "default": "Featured"
    },
    {
      "type": "text",
      "id": "hero_title",
      "label": "Title",
      "default": "The Future of Fashion: Sustainable Style Meets Innovation"
    },
    {
      "type": "textarea",
      "id": "hero_excerpt",
      "label": "Excerpt",
      "default": "Discover how leading designers are revolutionizing the industry with eco-friendly materials and cutting-edge technology."
    },
    {
      "type": "text",
      "id": "hero_author",
      "label": "Author",
      "default": "Priya Sharma"
    },
    {
      "type": "text",
      "id": "hero_date",
      "label": "Date",
      "default": "October 30, 2025"
    },
    {
      "type": "header",
      "content": "Secondary article 1"
    },
    {
      "type": "image_picker",
      "id": "secondary_1_image",
      "label": "Image"
    },
    {
      "type": "text",
      "id": "secondary_1_category",
      "label": "Category",
      "default": "Trends"
    },
    {
